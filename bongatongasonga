frt:Toggle("Teleport Fruit", function(bool)
    _G.WebAuto = false
    _G.Notif = false
    local spawn = game.workspace.AllspawnDF
local fspawn = game.workspace.AllspawnDF:GetChildren()
local me = game.Players.LocalPlayer.Character.HumanoidRootPart
local playern = game.Players.LocalPlayer.Name
if bool == false then
    _G.Notif = false
    end
if bool == true then
    _G.Notif = true
    while _G.Notif == true do
        wait()
        wait(1)
        _G.Notif = false
        _G.Notif = true
        if #spawn:GetChildren() == 1 then
            wait(0.1)
            if _G.WebAuto == true then
                print("yessir")
            me.CFrame = game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.CFrame
            else
                me.CFrame = game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.CFrame
            end
            
        if #spawn:GetChildren() == 0 then
            repeat
            wait(0.5) 
            until #spawn:GetChildren() == 1
            _G.WebAuto = true
            wait(0.1)
            if _G.WebAuto == true then
                print("yessir")
                if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
                else
                    me.CFrame = game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.CFrame
                end
            end
        end
    end
end
end
end)
