end)
aut:Dropdown("Auto Equip",{"--","Melee","Sword","Fruit"}, function(bool)
if bool == "--" then
_G.Mili = false 
_G.Swa = false
_G.Frat = false
end
if bool == "Melee" then
    _G.Mili = true 
    _G.Swa = false
    _G.Frat = false
end
if bool == "Sword" then
    _G.Mili = false 
    _G.Swa = true
    _G.Frat = false
end
if bool == "Fruit" then
    _G.Mili = false 
    _G.Swa = false
    _G.Frat = true
end
while _G.Mili == true do
wait()
for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
if v:IsA("Tool") then
if v.Name == "DarkLeg" or v.Name == "Cyborg" or v.Name == "FishmanKarate" or v.Name == "DragonClaw" or v.Name == "Electro" or v.Name == "None" then
    game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
end
end
end
end
while _G.Swa == true do
    wait()
    for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    if v:IsA("Tool") then
    if string.find(v.Name, "Blade") or v.Name == "Katana" or v.Name == "Pole" or v.Name == "Pipe" or v.Name == "Bisento" or v.Name == "Mini Mace" or v.Name == "Triple Katana" or v.Name == "Saber" or v.Name == "Hell Sword" or v.Name == "Pole" or v.Name == "Authentic Mace" or v.Name == "Anubis axe" or v.Name == "Adventure Knife" or v.Name == "Authentic Triple Katana" then
        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
    end
    end
    end
    end
while _G.Frat == true do
        wait()
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") then
                if not string.find(v.Name, "Fruit") and not string.find(v.Name, "Blade") and not string.find(v.Name, "DF") and not string.find(v.Name, "Ancient") and v.Name ~= "Katana" and v.Name ~= "Pole" and v.Name ~= "Pipe" and v.Name ~= "Bisento" and v.Name ~= "Mini Mace" and v.Name ~= "Triple Katana" and v.Name ~= "Saber" and v.Name ~= "Hell Swandd" and v.Name ~= "Pole" and v.Name ~= "Authentic Mace" and v.Name ~= "Anubis axe" and v.Name ~= "Adventure Knife" and v.Name ~= "Authentic Triple Katana" and v.Name ~= "DarkLeg" and v.Name ~= "Cybandg" and v.Name ~= "FishmanKarate" and v.Name ~= "DragonClaw" and v.Name ~= "Electro" and v.Name ~= "None" and v.Name ~= "LegacyPose" and v.Name ~= "Heart" and v.Name ~= "Fry Chicken" and v.Name ~= "Diamond" then
                    game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
            end
            end
            end
    end
end)
aut:Toggle("AutoAim", function(bool)
    local camera = game.Workspace.CurrentCamera
        local localplayer = game:GetService("Players").LocalPlayer
                function closestplayer()
                    local dist = math.huge
                    local target = nil
                    for i,v in pairs (game.workspace.Monster:GetDescendants()) do
                            if v:FindFirstChild("Head") and v.Humanoid.Health > 0 then
                                local magnitude = (v.Head.Position - localplayer.Character.Head.Position).magnitude
                                if magnitude < dist then
                                    dist = magnitude
                                    target = v
                                end
                                
                            end
                    end
                    return target
                end
    if bool == true then
        _G.Aimbott = true
end
    if bool == false then
    _G.Aimbott = false
    end
    while _G.Aimbott == true do
        wait()
        camera.CFrame = CFrame.new(camera.CFrame.Position,closestplayer().Head.Position)
    end
end)
