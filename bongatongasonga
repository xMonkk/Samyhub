frt:Toggle("Webhook", function(bool)
    local function leunco()
    if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").TextureId == "rbxassetid://5432757535" then
       marata = "Legendary"
    end
    if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").TextureId == "rbxassetid://5432757312" then
        marata = "Rare"
    end
    if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").TextureId == "rbxassetid://5432756947" then
        marata = "Uncommon"
    end
    if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").TextureId == "rbxassetid://5432756226" then
        marata = "Common"
    end
    end
    cooldown = 1
    _G.Webhook = false
    if bool == true then
        _G.Webhook = true
    while _G.Webhook == true do
        wait()
        if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") then
            if string.find(game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name, "Fruit") or string.find(game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name, "DF") then
                leunco()
            if cooldown == 1 then
                local url = blabla
                local data = {
                    ["content"] = "",
                    ["embeds"] = {
                        {
                            ["title"] = "**Fruit notifier**",
                            ["description"] = "Yo man you just grabbed " .. "**"..game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name.."**".." ("..marata..")",
                            ["type"] = "rich",
                            ["color"] = tonumber(0x7269da),
                        }
                    }
                 }
                 local newdata = game:GetService("HttpService"):JSONEncode(data)

local headers = {
   ["content-type"] = "application/json"
}
request = http_request or request or HttpPost or syn.request
local abcdef = {Url = url, Body = newdata, Method = "POST", Headers = headers}
request(abcdef)
                cooldown = 0
            end
        end
        end
        if game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool") == nil or string.find(game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name, "Fruit") == false or string.find(game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").Name, "DF") == false then
            cooldown = 1
        end
    end
    end
    if bool == false then
        _G.Webhook = false
    end
end)
