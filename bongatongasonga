misc:Button("Get Notifier", function()
        local car = game.workspace:FindFirstChild(game.Players.LocalPlayer.Name)
        while true do
            wait()
    local function notifier()
            if game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool") == nil then
                wait()
                game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = false
            end
            if game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool") then
        local spawn = game.workspace.AllspawnDF
        local l__LocalPlayer__20 = game.Players.LocalPlayer
        local u4 = game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle
        local l__HumanoidRootPart__9 = l__LocalPlayer__20.Character:WaitForChild("HumanoidRootPart")
        local l__TextLabel__27 = game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel
                        if game.Players.LocalPlayer.PlayerStats.Language.Value == "TH" then
                            game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = true
                            game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Text = "\224\184\156\224\184\165\224\185\132\224\184\161\224\185\137\224\184\163\224\184\176\224\184\162\224\184\176: " .. math.floor((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.Position).Magnitude) .. " \224\185\128\224\184\161\224\184\149\224\184\163";
                        elseif game.Players.LocalPlayer.PlayerStats.Language.Value == "US" and game.workspace:FindFirstChild(game.Players.LocalPlayer.Name) then
                            game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = true
                            game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Text = "Fruit : " .. math.floor((game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.Position).Magnitude) .. " away";
                    end
                    
                else
                    wait()
        end
            if game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool") == nil then
                wait()
                game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = false
            end
        end
            notifier()
            car.Humanoid.Died:Connect(function()
            wait(5)
            while true do
                wait()
            if game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool") == nil then
                wait()
                game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = false
            end
            if game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool") then
                if game.Players.LocalPlayer.PlayerStats.Language.Value == "TH" then
                    game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = true
                    game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Text = "\224\184\156\224\184\165\224\185\132\224\184\161\224\185\137\224\184\163\224\184\176\224\184\162\224\184\176: " .. math.floor((game.Players.LocalPlayer.Character.HumanoidRootPart.Position - game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.Position).Magnitude) .. " \224\185\128\224\184\161\224\184\149\224\184\163";
                elseif game.Players.LocalPlayer.PlayerStats.Language.Value == "US" and game.workspace:FindFirstChild(game.Players.LocalPlayer.Name) then
                    game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Visible = true
                    game.Players.LocalPlayer:WaitForChild("PlayerGui"):WaitForChild("Stats").TextLabel.Text = "Fruit : " .. math.floor((game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").Position - game.workspace.AllspawnDF:FindFirstChildWhichIsA("Tool").Handle.Position).Magnitude) .. " away";
            end
            
        else
            wait()
        end
    end
            end)
    
            end
           
end)
