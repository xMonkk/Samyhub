local misc = s:Tab("Misc")
misc:Button("Rejoin", function()
    game:GetService("TeleportService"):Teleport(game.PlaceId, game.Players.LocalPlayer)
end)
misc:Button("PlayTime", function()
    if game.workspace.DistributedGameTime <= 60 then
      ktaba = game.workspace.DistributedGameTime .. " second(s)"
    elseif game.workspace.DistributedGameTime >= 60 then
         ktaba = game.workspace.DistributedGameTime / 60 .. " minute(s)"
    elseif game.workspace.DistributedGameTime >= 3600 then
      ktaba = game.workspace.DistributedGameTime / 60 / 60 .. " hour(s)"
    end
    game:GetService("StarterGui"):SetCore("SendNotification", {Title = "You Have Played", Text = ktaba, Duration = 5})
end)
misc:Button("Server hop", function()
    
    if _G.SearchBest == true then
    local HttpService, TPService = game:GetService("HttpService"), game:GetService("TeleportService")

local Cursor = ""
local Attempt = 0
local FoundServer = false
local PlaceId = game.PlaceId
local CurrentId  = game.JobId
local BestServerID = ""
local BestServerPlayerAmount = math.huge
pcall(function()
while true do
if FoundServer == true then break end
Attempt = Attempt + 1
local ServersToTP = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?sortOrder=Asc&limit=100" .. Cursor))
Cursor = "&cursor=" .. ServersToTP.nextPageCursor
print("Attempt/CursorPage " .. tostring(Attempt) .. " Cursor: " .. tostring(ServersToTP.nextPageCursor))
for _, s in pairs(ServersToTP.data) do
    if s.playing < s.maxPlayers and s.id ~= CurrentId then
            if s.playing <= BestServerPlayerAmount then
        warn("Found Server With " .. tostring(s.playing) .. " Players Playing ID: " .. tostring(s.id))
        BestServerPlayerAmount = s.playing
        BestServerID = s.id
        if BestServerPlayerAmount <= 1 then
        FoundServer = true
        end
    end
    end
end
wait(0.1)
end
end)
TPService:TeleportToPlaceInstance(game.PlaceId, BestServerID)
end
if _G.SearchBest == false then
    local HttpService, TPService = game:GetService("HttpService"), game:GetService("TeleportService")

local Cursor = ""
local Attempt = 0
local FoundServer = false
local PlaceId = game.PlaceId
local CurrentId  = game.JobId
local BestServerID = ""
local BestServerPlayerAmount = math.huge
pcall(function()
while true do
if FoundServer == true then break end
Attempt = Attempt + 1
local ServersToTP = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/" .. PlaceId .. "/servers/Public?sortOrder=Asc&limit=100" .. Cursor))
Cursor = "&cursor=" .. ServersToTP.nextPageCursor
print("Attempt/CursorPage " .. tostring(Attempt) .. " Cursor: " .. tostring(ServersToTP.nextPageCursor))
for _, s in pairs(ServersToTP.data) do
    if Attempt == "7" then
    if s.playing < s.maxPlayers and s.id ~= CurrentId then
            if s.playing <= BestServerPlayerAmount then
        warn("Found Server With " .. tostring(s.playing) .. " Players Playing ID: " .. tostring(s.id))
        BestServerPlayerAmount = s.playing
        BestServerID = s.id
        if BestServerPlayerAmount <= 1 then
        FoundServer = true
        end
    end
    end
    end
end
wait(0.1)
end
end)
TPService:TeleportToPlaceInstance(game.PlaceId, BestServerID)
end
end)
