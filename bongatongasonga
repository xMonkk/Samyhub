    contra = 1
    local function activate()
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v:IsA("Tool") then
                v:activate()
            end
        end
      end
if bool == "None" then
    _G.piwpiw = false
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.X,game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y+50,game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Z)
    if game.Players.LocalPlayer.Character.Haki == 1 then
    game.Players.LocalPlayer.Character.Services.Client.Armament:FireServer()
    end
end
if bool == "Soldier [Lv. 1]" then
    mobtofarm = "Soldier [Lv. 1]"
    _G.piwpiw = true
    game.RunService.Stepped:Connect(
      function()
          if _G.piwpiw == true then
          game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
          for i, v in pairs(workspace.Monster.Mon:GetChildren()) do
              if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v.Name == mobtofarm then
                  repeat
                      wait()
                      if _G.piwpiw == true then
                        activate()
                      game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                          v.HumanoidRootPart.CFrame * CFrame.new(0, -6, 0)
                      game:GetService("RunService").Heartbeat:wait()
                      end
                      if _G.piwpiw == false then
                          return
                          end
                  until _G.Autofarm == false
              end
              end
          end
      end
    )
end

